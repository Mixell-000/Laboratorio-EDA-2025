import java.util.*;
import java.io.*;

class Movie {
	//atributos
	String title;
	String directores;
	String genre;
	int releaseYear;
	double rating;


	//Metodos
	Movie(String title, String director, String genre, int releaseYear, double rating) {
        this.title = title;
        this.director = director;
        this.genre = genre;
        this.releaseYear = releaseYear;
        this.rating = rating;
    }

}

